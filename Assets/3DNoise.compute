#pragma kernel CSMain

RWStructuredBuffer<float> Result;
RWStructuredBuffer<float3> octaves;
int3 size;
float scale;
float persistance;
float lacunarity;
float noiseExtents;
float3 worldOffset;

[numthreads(16,64,1)]
void CSMain(uint3 id : SV_DispatchThreadID)
{
    Result[(size.x * size.y * id.z) + (size.y * id.y) + id.x] = octaves[3].x;
}
